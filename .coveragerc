[run]
# Configuração do Coverage
source = apps
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__init__.py
    */settings.py
    */wsgi.py
    */asgi.py
    */manage.py

[report]
# Relatórios
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
